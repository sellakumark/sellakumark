{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "sellakumark": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/sellakumark",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "node_modules/aos/dist/aos.css",
              "node_modules/boxicons/css/boxicons.min.css",
              "node_modules/glightbox/dist/css/glightbox.min.css",
              "node_modules/swiper/swiper.min.css",
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/aos/dist/aos.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "node_modules/glightbox/dist/js/glightbox.min.js",
              "node_modules/isotope-layout/dist/isotope.pkgd.min.js",
              "node_modules/swiper/swiper-bundle.min.js",
              "node_modules/typed.js/lib/typed.min.js",
              "node_modules/@srexi/purecounterjs/js/purecounter_vanilla.js",
              "node_modules/waypoints/lib/noframework.waypoints.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "512kb",
                  "maximumError": "512kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "sellakumark:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "sellakumark:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sellakumark:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "sellakumark:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sellakumark:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "sellakumark",
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
